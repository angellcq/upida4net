<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="UpidaExample" namespace="UpidaExample.Domain">
  <class name="OrderItem">
    <id name="Id">
      <generator class="sequence">
        <param name="sequence">orderitem_id_seq</param>
      </generator>
    </id>
    <property name="ProductId" />
    <property name="Count" />
    <property name="Price" />
    <many-to-one name="Order" column="order_id" class="Order" />
  </class>
</hibernate-mapping>