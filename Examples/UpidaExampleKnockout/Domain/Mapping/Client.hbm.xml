<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="UpidaExampleKnockout" namespace="UpidaExampleKnockout.Domain">
	<class name="Client">
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="Name" />
		<set name="Orders" table="Orders">
			<key column="client_id" />
			<one-to-many class="Order" />
		</set>
	</class>
</hibernate-mapping>